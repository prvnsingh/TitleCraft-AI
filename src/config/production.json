{
  "llm": {
    "provider": "openai",
    "model": "gpt-4",
    "temperature": 0.7,
    "max_tokens": 2000,
    "timeout": 60,
    "retry_attempts": 5
  },
  "database": {
    "host": "postgres",
    "port": 5432,
    "database": "titlecraft",
    "username": "titlecraft_user",
    "pool_size": 20
  },
  "redis": {
    "host": "redis",
    "port": 6379,
    "database": 0,
    "ttl": 3600
  },
  "api": {
    "host": "0.0.0.0",
    "port": 8000,
    "debug": false,
    "cors_origins": ["https://titlecraft.ai"],
    "rate_limit": 100
  },
  "data": {
    "data_directory": "/app/data",
    "embeddings_model": "all-MiniLM-L6-v2",
    "max_similar_examples": 5,
    "min_examples_for_profile": 10,
    "cache_profiles": true
  }
}
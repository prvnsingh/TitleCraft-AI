{
  "llm": {
    "provider": "openai",
    "model": "gpt-4",
    "temperature": 0.7,
    "max_tokens": 2000,
    "timeout": 30,
    "retry_attempts": 3
  },
  "database": {
    "host": "localhost",
    "port": 5432,
    "database": "titlecraft_dev",
    "username": "dev_user",
    "pool_size": 5
  },
  "redis": {
    "host": "localhost",
    "port": 6379,
    "database": 0,
    "ttl": 1800
  },
  "api": {
    "host": "127.0.0.1",
    "port": 8000,
    "debug": true,
    "cors_origins": ["http://localhost:3000", "http://localhost:8080"],
    "rate_limit": 200
  },
  "data": {
    "data_directory": "data",
    "embeddings_model": "all-MiniLM-L6-v2",
    "max_similar_examples": 5,
    "min_examples_for_profile": 5,
    "cache_profiles": true
  }
}